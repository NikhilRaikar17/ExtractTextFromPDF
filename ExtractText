import PyPDF2 pdfFileObj = open('w.pdf', 'rb') pdfReader = PyPDF2.PdfFileReader(pdfFileObj)   def ExtractPdf():     # Put all the text into one single variable.     details_page = []     render_pdf(details_page)     # Create a example words list(Please add all the related keywords needed)     words_list = ["Abstract", "Introduction", "Related", "Conclusion"]     print("There are", len(words_list), "in the words list")     str = " "     details = str.join(details_page)     words = details.split()     place = []     append_list(place, words, words_list)     print(list(zip(words_list, place)))     final_array = list(zip(words_list, place))     final_array.sort()     print("Sorting the final array")     print(final_array)     print("Extracting the relevant texts from pdf")     print(" ")     listint = final_array[0]     list2int = final_array[1]     counter = 0     Extract_Info(counter, details, final_array, list2int, listint)   def render_pdf(details_page):     for each in range(pdfReader.numPages):         pageObj = pdfReader.getPage(each)         variable = pageObj.extractText()         details_page.append(variable)   def append_list(place, words, words_list):     # Enumerate and create a rank between the keywords.     for c, a in enumerate(words):         for b in words_list:             if b == a and b not in place:                 place.append(words.index(a))             elif b not in words:                 words_list.remove(b)                 print("The word", b, "was not found in the pdf file")   def Extract_Info(counter, details, final_array, list2int, listint):     for each in (final_array):          if counter < len(final_array) - 2:             new = (details.split(listint[0])[1].split(list2int[0])[0].rsplit())             print(listint[0], ":", [' '.join(new)])             print(" ")             counter = counter + 1             listint = final_array[0 + counter]             list2int = final_array[1 + counter]          elif counter < len(final_array) - 1:             new = (details.split(final_array[counter][0])[1].split(final_array[counter + 1][0])[0].rsplit())             print(final_array[counter][0], ":", [' '.join(new)])             print(" ")             counter = counter + 1         else:             new = (details.split(final_array[counter][0])[1].rsplit())             print(final_array[counter][0], ":", [' '.join(new)])             print(" ")   ExtractPdf()